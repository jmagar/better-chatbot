{
  "name": "better-chatbot",
  "version": "1.25.0",
  "private": true,
  "author": "cgoinglove",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next dev",
    "dev:turbopack": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next dev --turbopack",
    "dev:https": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next dev --experimental-https",
    "build": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next build",
    "start": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next start",
    "build:local": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp NO_HTTPS='1' next build",
    "test": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp vitest run",
    "test:watch": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp vitest",
    "test:e2e": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp playwright test",
    "test:e2e:ui": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp playwright test --ui",
    "test:e2e:seed": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp dotenv -- tsx scripts/seed-test-users.ts",
    "test:e2e:clean": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp dotenv -- tsx scripts/cleanup-all-test-data.ts",
    "lint": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next lint && biome lint",
    "lint:fix": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp next lint --fix && biome lint --write --unsafe",
    "format": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp biome format --write",
    "check-types": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsc --noEmit",
    "initial:env": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/initial-env.ts",
    "openai-compatiable:init": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/init-openai-compatiable.ts",
    "openai-compatiable:parse": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/parse-openai-compatiable.ts",
    "postinstall": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/postinstall.ts",
    "clean": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/clean.ts",
    "clean:next": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/clean.ts .next",
    "mcp:export": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/export-mcp-config.ts",
    "db:generate": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit generate",
    "db:reset": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit drop && drizzle-kit push",
    "db:push": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit push",
    "db:studio": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit studio",
    "db:migrate": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp tsx scripts/db-migrate.ts",
    "db:pull": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit pull",
    "db:check": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp drizzle-kit check",
    "docker-compose:up": "docker-compose -f docker/compose.yml up -d --build",
    "docker-compose:down": "docker-compose -f docker/compose.yml down",
    "docker-compose:logs": "docker-compose -f docker/compose.yml logs -f",
    "docker-compose:ps": "docker-compose -f docker/compose.yml ps",
    "docker-compose:update": "git pull && docker-compose -f docker/compose.yml up -d --build",
    "docker:pg": "docker run --name better-chatbot-pg -e POSTGRES_PASSWORD=your_password -e POSTGRES_USER=your_username -e POSTGRES_DB=your_database_name -p 5432:5432 -d postgres",
    "docker:redis": "docker run --name better-chatbot-redis -p 6379:6379 -d redis:7-alpine",
    "docker:app": "docker build -f docker/Dockerfile -t better-chatbot . && docker run -p 3000:3000 -e NO_HTTPS=1 better-chatbot",
    "prepare": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp husky",
    "playwright:install": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp playwright install",
    "check": "cross-env NODE_COMPILE_CACHE=.cache/node-compile-cache TMPDIR=.cache/tmp pnpm lint:fix && pnpm check-types && pnpm test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.39",
